    "b": {
        "prefix": "b",
        "body": [
            "b\t${1:target}\t\t\t# branch to $1"
        ],
        "description": "branch uncodnitional",
        "scope": "source.mips"
    },
    "div": {
        "prefix": "div",
        "body": [
            "div\t${1:\\$t0}, ${2:\\$t1}\t\t# $1 / $2",
            "mflo\t${3:\\$t2}\t\t\t# $3 = floor($1 / $2)",
            "mfhi\t${4:\\$t3}\t\t\t# $4 = $1 mod $2"
        ],
        "description": "divide",
        "scope": "source.mips"
    },
    "jal": {
        "prefix": "jal",
        "body": [
            "jal\t${1:target}\t\t\t# jump to $1 and save position to \\$ra"
        ],
        "description": "jump and link",
        "scope": "source.mips"
    },
    "jr": {
        "prefix": "jr",
        "body": [
            "jr\t${1:\\$ra}\t\t\t# jump to $1"
