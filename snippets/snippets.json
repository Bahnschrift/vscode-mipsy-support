{
    "print int": {
        "prefix": ["print", "int"],
        "body": [
            "li\t\\$v0, 1\t\t\t# system call #1 - print int",
            "la\t\\$a0, ${1:variable}",
            "syscall\t\t\t\t# execute"
        ],
        "description": "print int",
        "scope": "source.mips"
    },
    "print float": {
        "prefix": ["print", "float"],
        "body": [
            "li\t\\$v0, 2\t\t\t# system call #2 - print float",
            "la\t\\$a0, ${1:variable}",
            "syscall\t\t\t\t# execute"
        ],
        "description": "print float",
        "scope": "source.mips"
    },
    "print double": {
        "prefix": ["print", "double"],
        "body": [
            "li\t\\$v0, 3\t\t\t# system call #3 - print double",
            "la\t\\$a0, ${1:variable}",
            "syscall\t\t\t\t# print $1"
        ],
        "description": "print double",
        "scope": "source.mips"
    },
    "print string": {
        "prefix": ["print", "string"],
        "body": [
            "li\t\\$v0, 4\t\t\t# system call #4 - print string",
            "la\t\\$a0, ${1:string}",
            "syscall\t\t\t\t# print $1"
        ],
        "description": "print string",
        "scope": "source.mips"
    },
    "print char": {
        "prefix": ["print", "char"],
        "body": [
            "li\t\\$v0, 11\t\t\t# system call #11 - print character",
            "la\t\\$a0, ${1:variable}",
            "syscall\t\t\t\t# print $1"
        ],
        "description": "print char",
        "scope": "source.mips"
    },
    "print newline": {
        "prefix": ["print", "newline"],
        "body": [
            "li\t\\$v0, 11\t\t\t# system call #4 - print character",
            "li\t\\$a0, '\\n'",
            "syscall\t\t\t\t# print newline"
        ],
        "description": "print newline",
        "scope": "source.mips"
    },
    "read int": {
        "prefix": ["read", "input", "int"],
        "body": [
            "li\t\\$v0, 5\t\t\t# system call #5 - read int",
            "syscall\t\t\t\t# read int",
            "move\t${1:\\$t0}, \\$v0\t\t# move return value to $1"
        ],
        "description": "read integer from user",
        "scope": "source.mips"
    },
    "read float": {
        "prefix": ["read", "input", "float"],
        "body": [
            "li\t\\$v0, 6\t\t\t# system call #6 - read float",
            "syscall\t\t\t\t# read float",
            "move\t${1:\\$t0}, \\$f0\t\t# move return value to $1"
        ],
        "description": "read float from user",
        "scope": "source.mips"
    },
    "read double": {
        "prefix": ["read", "input", "double"],
        "body": [
            "li\t\\$v0, 7\t\t\t# system call #7 - read double",
            "syscall\t\t\t\t# read double",
            "move\t${1:\\$t0}, \\$f0\t\t# move upper return value to $1",
            "move\t${2:\\$t1}, \\$f1\t\t# move lower return value to $2"
        ]
    },
    "read string": {
        "prefix": ["read", "input", "string"],
        "body": [
            "li\t\\$v0, 8\t\t\t# system call #8 - read string",
            "syscall\t\t\t\t# read string",
            "move\t${1:\\$t0}, \\$a0\t\t# move return pointer to $1",
            "move\t${2:\\$t1}, \\$a1\t\t# move return length to $2"
        ],
        "description": "read string from user",
        "scope": "source.mips"
    },
    "read char": {
        "prefix": ["read", "input", "char"],
        "body": [
            "li\t\\$v0, 12\t\t\t# system call #12 - read char",
            "syscall\t\t\t\t# read char",
            "move\t${1:\\$t0}, \\$v0\t\t# move return value to $1"
        ],
        "description": "read char from user",
        "scope": "source.mips"
    },
    "return 0": {
        "prefix": ["return", "exit", "stop"],
        "body": [
            "li\t\\$v0, 0\t\t\t# return 0",
            "jr\t\\$ra"
        ],
        "description": "exit program and return 0",
        "scope": "source.mips"
    },
    "addi": {
        "prefix": "addi",
        "body": [
            "addi\t${1:\\$t0}, ${2:\\$t1}, ${3:0}\t\t# $1 = $2 + $3"
        ],
        "description": "add immediate",
        "scope": "source.mips"
    },
    "add": {
        "prefix": "add",
        "body": [
            "add\t${1:\\$t0}, ${2:\\$t1}, ${3:\\$t2}\t\t# $1 = $2 + $3"
        ],
        "description": "add",
        "scope": "source.mips"
    },
    "and": {
        "prefix": "and",
        "body": [
            "and\t${1:\\$t0}, ${2:\\$t1}, ${3:\\$t2}\t\t# $1 = $2 & $3"
        ],
        "description": "and",
        "scope": "source.mips"
    },
    "andi": {
        "prefix": "andi",
        "body": [
            "andi\t${1:\\$t0}, ${2:\\$t1}, ${3:0}\t\t# $1 = $2 & $3"
        ],
        "description": "and immediate",
        "scope": "source.mips"
    },
    "beq": {
        "prefix": "beq",
        "body": [
            "beq\t${1:\\$t0}, ${2:\\$t1}, ${3:target}\t# if $1 == $2 goto $3"
        ],
        "description": "branch if equal",
        "scope": "source.mips"
    },
    "bge": {
        "prefix": "bge",
        "body": [
            "bge\t${1:\\$t0}, ${2:\\$t1}, ${3:target}\t# if $1 >= $2 goto $3"
        ],
        "description": "branch if greater than or equal",
        "scope": "source.mips"
    },
    "bgt": {
        "prefix": "bgt",
        "body": [
            "bgt\t${1:\\$t0}, ${2:\\$t1}, ${3:target}\t# if $1 > $2 goto $3"
        ],
        "description": "branch if greater than",
        "scope": "source.mips"
    },
    "ble": {
        "prefix": "ble",
        "body": [
            "ble\t${1:\\$t0}, ${2:\\$t1}, ${3:target}\t# if $1 <= $2 goto $3"
        ],
        "description": "branch if less than or equal",
        "scope": "source.mips"
    },
    "blt": {
        "prefix": "blt",
        "body": [
            "blt\t${1:\\$t0}, ${2:\\$t1}, ${3:target}\t# if $1 < $2 goto $3"
        ],
        "description": "branch if less than",
        "scope": "source.mips"
    },
    "bne": {
        "prefix": "bne",
        "body": [
            "bne\t${1:\\$t0}, ${2:\\$t1}, ${3:target}\t# if $1 != $2 goto $3"
        ],
        "description": "branch if not equal",
        "scope": "source.mips"
    },
    "b": {
        "prefix": "b",
        "body": [
            "b\t${1:target}\t\t\t# branch to $1"
        ],
        "description": "branch unconditional",
        "scope": "source.mips"
    },
    "div": {
        "prefix": "div",
        "body": [
            "div\t${1:\\$t0}, ${2:\\$t1}\t\t# $1 / $2",
            "mflo\t${3:\\$t2}\t\t\t# $3 = floor($1 / $2)",
            "mfhi\t${4:\\$t3}\t\t\t# $4 = $1 mod $2"
        ],
        "description": "divide",
        "scope": "source.mips"
    },
    "jal": {
        "prefix": "jal",
        "body": [
            "jal\t${1:target}\t\t\t# goto $1 and save position to \\$ra"
        ],
        "description": "jump and link",
        "scope": "source.mips"
    },
    "jr": {
        "prefix": "jr",
        "body": [
            "jr\t${1:\\$ra}\t\t\t# goto $1"
        ],
        "description": "jump register",
        "scope": "source.mips"
    },
    "j": {
        "prefix": "j",
        "body": [
            "j\t${1:target}\t\t\t# goto $1"
        ],
        "description": "jump",
        "scope": "source.mips"
    },
    "la": {
        "prefix": "la",
        "body": [
            "la\t${1:\\$t1}, ${2:var1}\t\t# load address of $2 into $1"
        ],
        "description": "load address",
        "scope": "source.mips"
    },
    "lb": {
        "prefix": "lb",
        "body": [
            "lb\t${1:\\$t1}, ${2:${3:0}(${4:\\$s1})}\t\t# load byte from $2 into $1"
        ],
        "description": "load byte",
        "scope": "source.mips"
    },
    "li": {
        "prefix": "li",
        "body": [
            "li\t${1:\\$t1}, ${2:0}\t\t\t# $1 = $2"
        ],
        "description": "load immediate",
        "scope": "source.mips"
    },
    "lw": {
        "prefix": "lw",
        "body": [
            "lw\t${1:\\$t1}, ${2:${3:0}(${4:\\$s1})}\t\t# load word from $2 into $1"
        ],
        "description": "load word",
        "scope": "source.mips"
    },
    "move": {
        "prefix": "move",
        "body": [
            "move\t${1:\\$t0}, ${2:\\$t1}\t\t# $1 = $2"
        ],
        "description": "move",
        "scope": "source.mips"
    },
    "mult": {
        "prefix": "mult",
        "body": [
            "mult\t${1:\\$t0}, ${2:\\$t1}\t\t# $1 * $2 = Hi and Lo registers",
            "mflo\t${3:\\$t2}\t\t\t# copy Lo to $3"
        ],
        "description": "multiply",
        "scope": "source.mips"
    },
    "nor": {
        "prefix": "nor",
        "body": [
            "nor\t${1:\\$t0}, ${2:\\$t1}, ${3:\\$t2}\t\t# $1 = ~($2 | $3)"
        ],
        "description": "nor",
        "scope": "source.mips"
    },
    "or": {
        "prefix": "or",
        "body": [
            "or\t${1:\\$t0}, ${2:\\$t1}, ${3:\\$t2}\t\t# $1 = $2 | $3"
        ],
        "description": "or",
        "scope": "source.mips"
    },
    "ori": {
        "prefix": "ori",
        "body": [
            "ori\t${1:\\$t0}, ${2:\\$t1}, ${3:0}\t\t\t# $1 = $2 | $3"
        ],
        "description": "or immediate",
        "scope": "source.mips"
    },
    "sb": {
        "prefix": "sb",
        "body": [
            "sb\t${1:\\$t1}, ${2:${3:0}(${4:\\$s1})}\t\t# store $1 at $2"
        ],
        "description": "store byte",
        "scope": "source.mips"
    },
    "sll": {
        "prefix": "sll",
        "body": [
            "sll\t${1:\\$t0}, ${2:\\$t1}, ${3:0}\t\t# $1 = $2 << $3"
        ],
        "description": "shift left",
        "scope": "source.mips"
    },
    "slt": {
        "prefix": "slt",
        "body": [
            "slt\t${1:\\$t0}, ${2:\\$s0}, ${3:\\$s1}\t\t# $1 = ($2 < $3) ? 1 : 0"
        ],
        "description": "set on less than",
        "scope": "source.mips"
    },
    "slti": {
        "prefix": "slti",
        "body": [
            "slti\t${1:\\$t0}, ${2:\\$s0}, ${3:0}\t\t# $1 = ($2 < $3) ? 1 : 0"
        ],
        "description": "set on less than immediate",
        "scope": "source.mips"
    },
    "srl": {
        "prefix": "srl",
        "body": [
            "srl\t${1:\\$t0}, ${2:\\$t1}, ${3:0}\t\t# $1 = $2 >> $3"
        ],
        "description": "shift right",
        "scope": "source.mips"
    },
    "sw": {
        "prefix": "sw",
        "body": [
            "sw\t${1:\\$t1}, ${2:${3:0}(${4:\\$s1})}\t\t# store $1 at $2"
        ],
        "description": "store word",
        "scope": "source.mips"
    },
    "string": {
        "prefix": "string",
        "body": [
            "${1:string}:\t.asciiz\t\"$2\""
        ],
        "description": "string var",
        "scope": "source.mips"
    },
    "sub": {
        "prefix": "sub",
        "body": [
            "sub\t${1:\\$t0}, ${2:\\$t1}, ${3:\\$t2}\t\t# $1 = $2 - $3"
        ],
        "description": "subtract",
        "scope": "source.mips"
    },
    "subi": {
        "prefix": "subi",
        "body": [
            "subi\t${1:\\$t0}, ${2:\\$t1}, ${3:0}\t\t# $1 = $2 - $3"
        ],
        "description": "subtract immediate",
        "scope": "source.mips"
    },
    "var": {
        "prefix": "var",
        "body": [
            "${1:var}:\t.${2:word}\t$3"
        ],
        "description": "variable",
        "scope": "source.mips"
    },
    "xor": {
        "prefix": "xor",
        "body": [
            "xor\t${1:\\$t0}, ${2:\\$t1}, ${3:\\$t2}\t\t# $1 = $2 ^ $3"
        ],
        "description": "xor",
        "scope": "source.mips"
    },
    "xori": {
        "prefix": "xori",
        "body": [
            "xori\t${1:\\$t0}, ${2:\\$t1}, ${3:0}\t\t# $1 = $2 ^ $3"
        ],
        "description": "xor immediate",
        "scope": "source.mips"
    },
    "begin": {
        "prefix": "begin",
        "body": [
            "begin\t\t\t\t# move frame pointer"
        ],
        "description": "begin - move frame pointer",
        "scope": "source.mips"
    },
    "end": {
        "prefix": "end",
        "body": [
            "end\t\t\t\t# move frame pointer back"
        ],
        "description": "end - move frame pointer back",
        "scope": "source.mips"
    },
    "push": {
        "prefix": "push",
        "body": [
            "push\t${1:\\$t0}\t\t\t# push $1 to the stack"
        ],
        "description": "push",
        "scope": "source.mips"
    },
    "pop": {
        "prefix": "pop",
        "body": [
            "pop\t${1:\\$t0}\t\t\t# pop from the stack into $1"
        ],
        "description": "pop",
        "scope": "source.mips"
    },
    "syscall": {
        "prefix": "syscall",
        "body": [
            "syscall"
        ],
        "description": "syscall",
        "scope": "source.mips"
    }
}