{
    "print_int": {
        "prefix": "print",
        "body": [
            "li\t\\$v0, 1\t\t\t# system call #1 - print int",
            "la\t\\$a0, ${1:variable}",
            "syscall\t\t\t\t# execute"
        ],
        "description": "print int",
        "scope": "source.mips"
    },
    "print_float": {
        "prefix": "print",
        "body": [
            "li\t\\$v0, 2\t\t\t# system call #2 - print float",
            "la\t\\$a0, ${1:variable}",
            "syscall\t\t\t\t# execute"
        ],
        "description": "print float",
        "scope": "source.mips"
    },
    "print_double": {
        "prefix": "print",
        "body": [
            "li\t\\$v0, 3\t\t\t# system call #3 - print double",
            "la\t\\$a0, ${1:variable}",
            "syscall\t\t\t\t# print $1"
        ],
        "description": "print double",
        "scope": "source.mips"
    },
    "print_string": {
        "prefix": "print",
        "body": [
