{
    "name": "mips",
    "displayName": "MIPS Support",
    "description": "Provides basic syntax highlighting for MIPS assembly language",
    "version": "0.0.1",
    "publisher": "darkhan",
    "engines": {
        "vscode": "^1.5.0"
    },
    "categories": [
        "Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "mips",
            "aliases": ["MIPS Assembly", "mips"],
            "extensions": [".s",".mips",".spim",".asm"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "mips",
            "scopeName": "source.mips",
            "path": "./syntaxes/mips.tmLanguage"
        }],
        "snippets": [
            {
                "language": "mips",
                "path": "./snippets/snippets.json"
            }
        ]
    }
}